#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/qrcode-terminal@0.12.0/node_modules/qrcode-terminal/bin/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/qrcode-terminal@0.12.0/node_modules/qrcode-terminal/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/qrcode-terminal@0.12.0/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/qrcode-terminal@0.12.0/node_modules/qrcode-terminal/bin/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/qrcode-terminal@0.12.0/node_modules/qrcode-terminal/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/qrcode-terminal@0.12.0/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/qrcode-terminal.js" "$@"
else
  exec node  "$basedir/../../bin/qrcode-terminal.js" "$@"
fi
