#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/@slidev+cli@0.40.3_postcss@8.4.21/node_modules/@slidev/cli/bin/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/@slidev+cli@0.40.3_postcss@8.4.21/node_modules/@slidev/cli/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/@slidev+cli@0.40.3_postcss@8.4.21/node_modules/@slidev/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/@slidev+cli@0.40.3_postcss@8.4.21/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/@slidev+cli@0.40.3_postcss@8.4.21/node_modules/@slidev/cli/bin/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/@slidev+cli@0.40.3_postcss@8.4.21/node_modules/@slidev/cli/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/@slidev+cli@0.40.3_postcss@8.4.21/node_modules/@slidev/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/@slidev+cli@0.40.3_postcss@8.4.21/node_modules:/home/evilscript/.local/share/Cryptomator/mnt/Sync/Magistrale/Tesi/slides-tesi/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@slidev/cli/bin/slidev.js" "$@"
else
  exec node  "$basedir/../@slidev/cli/bin/slidev.js" "$@"
fi
